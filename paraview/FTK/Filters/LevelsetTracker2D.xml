<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="LevelsetTracker2D" class="ftkLevelsetTracker2D" label="LevelsetTracker2D">
      <Documentation
         short_help="track levelsets in 2D time-varying scalar field data"
         long_help="track levelsets in 2D time-varying scalar field data">
      </Documentation>
      
      <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
      </InputProperty>
      
      <DoubleVectorProperty
        name="Threshold"
        command="SetThreshold"
        number_of_elements="1"
        default_values="0.0">
      </DoubleVectorProperty>

      <StringVectorProperty 
        name="InputVariable"
        command="SetInputVariable"
        number_of_elements="1"
        animateable="1">
        <ArrayListDomain name="array_list"
          attribute_type="Scalars"
          input_domain_name="inputs_array">
          <RequiredProperties>
            <Property name="Input"
              function="Input" />
          </RequiredProperties>
        </ArrayListDomain>
      </StringVectorProperty>

      <Hints>
        <ShowInMenu category="FTK"/>
      </Hints>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
